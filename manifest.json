{
    "name": "Purify",
    "version": "0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "18",
    "description": "An article filterer",
    "background": {"scripts": ["js/background.js"]},
    "browser_action": {
        "default_icon": "images/purify-19.png"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/test.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}